N8N_HOST= subdomain.domain.com # without https:// 
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_EDITOR_BASE_URL="https://subdomain.domain.com:8444" # mine is on 8444 because I use a reverse proxy with ssl
N8N_PATH=/
N8N_PROXY_HOPS=1
N8N_PUSH_BACKEND=websocket

NODE_ENV=production
GENERIC_TIMEZONE=Europe/Berlin # set your timezone here - https://en.wikipedia.org/wiki/List_of_tz_database_time_zones


N8N_DIAGNOSTICS_ENABLED=false
N8N_TEMPLATES_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false
